# Discord Bot Token (from Discord Developer Portal)
DISCORD_TOKEN=your_discord_bot_token_here

# Anthropic API Key (for Claude Code)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# GitHub Personal Access Token (with repo and issue scopes)
GH_TOKEN=your_github_personal_access_token_here

# GitHub Repository (format: owner/repo-name)
GITHUB_REPO=owner/repo-name

# Allowed Discord Server IDs (comma-separated list of server IDs)
# If empty, all servers will be allowed (not recommended for production)
ALLOWED_DISCORD_SERVERS=123456789012345678,987654321098765432